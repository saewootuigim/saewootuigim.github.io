file = {'email_128x128.png', 'github_128x128.png', 'googlescholar_500x500.png', 'linkedin_128x128.png', 'youtube_128x128.png'};
l = length(file);

for i = 1 : l
    a = imread(file{i});
    b = rgb2gray(a);
    C = strsplit(file{i},'.');
    imwrite(b,[C{1},'_gray.',C{2}]);
    
    a = imread([C{1},'_gray.',C{2}]);
    a(a<120) = 120;
    imwrite()
end

